{
  "name": "okx-wasmy",
  "displayName": "OKX Wasmy",
  "description": "wibbly wobbly okx wasmy tool to interact with cosmwasm smart contracts",
  "version": "0.1.14",
  "preview": false,
  "publisher": "okx-com",
  "icon": "media/icon.png",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "https://github.com/spoo-bar/okx-wasmy",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Testing",
    "Notebooks",
    "Other"
  ],
  "keywords": [
    "Cosmos",
    "Cosmwasm",
    "Juno",
    "Smart Contract",
    "Archway",
    "Chihuahua",
    "Stargaze",
    "Neutron",
    "OKTC"
  ],
  "activationEvents": [
    "onView:account",
    "onView:contract",
    "onView:query",
    "onView:execute",
    "onView:sign",
    "onLanguage:rust",
    "onNotebook:cw-notebook"
  ],
  "main": "./dist/extension.js",
  "l10n": "./l10n",
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": "limited",
      "description": "%okx-wasmy.capabilities.trustedWorkspace.description%"
    }
  },
  "contributes": {
    "commands": [
      {
        "command": "okx-wasmy.addAccount",
        "title": "%okx-wasmy.addAccount.title%",
        "category": "Okx Wasmy",
        "icon": "$(add)"
      },
      {
        "command": "okx-wasmy.requestFunds",
        "title": "%okx-wasmy.requestFunds.title%",
        "category": "Okx Wasmy",
        "enablement": "viewItem == account",
        "icon": {
          "light": "media/icons/light/fund.svg",
          "dark": "media/icons/dark/fund.svg"
        }
      },
      {
        "command": "okx-wasmy.openInExplorer",
        "title": "%okx-wasmy.openInExplorer.title%",
        "category": "Okx Wasmy",
        "enablement": "viewItem == account",
        "icon": "$(link-external)"
      },
      {
        "command": "okx-wasmy.copyAddress",
        "title": "%okx-wasmy.copyAddress.title%",
        "category": "Okx Wasmy",
        "enablement": "viewItem == account || viewItem == contract"
      },
      {
        "command": "okx-wasmy.copyMnemonic",
        "title": "%okx-wasmy.copyMnemonic.title%",
        "category": "Okx Wasmy",
        "enablement": "viewItem == account"
      },
      {
        "command": "okx-wasmy.deleteAccount",
        "title": "%okx-wasmy.deleteAccount.title%",
        "category": "Okx Wasmy",
        "enablement": "viewItem == account"
      },
      {
        "command": "okx-wasmy.refreshAccount",
        "title": "%okx-wasmy.refreshAccount.title%",
        "category": "Okx Wasmy",
        "icon": "$(refresh)"
      },
      {
        "command": "okx-wasmy.addContract",
        "title": "%okx-wasmy.addContract.title%",
        "category": "Okx Wasmy",
        "icon": "$(add)"
      },
      {
        "command": "okx-wasmy.deleteContract",
        "title": "%okx-wasmy.deleteContract.title%",
        "category": "Okx Wasmy",
        "enablement": "viewItem == contract"
      },
      {
        "command": "okx-wasmy.updateContractAdmin",
        "title": "%okx-wasmy.updateContractAdmin.title%",
        "category": "Okx Wasmy",
        "enablement": "viewItem == contract"
      },
      {
        "command": "okx-wasmy.clearContractAdmin",
        "title": "%okx-wasmy.clearContractAdmin.title%",
        "category": "Okx Wasmy",
        "enablement": "viewItem == contract"
      },
      {
        "command": "okx-wasmy.getContractChecksum",
        "title": "%okx-wasmy.getContractChecksum.title%",
        "category": "Okx Wasmy",
        "enablement": "viewItem == contract"
      },
      {
        "command": "okx-wasmy.downloadContractBinary",
        "title": "%okx-wasmy.downloadContractBinary.title%",
        "category": "Okx Wasmy",
        "enablement": "viewItem == contract"
      },
      {
        "command": "okx-wasmy.reloadConfig",
        "title": "%okx-wasmy.reloadConfig.title%",
        "category": "Okx Wasmy"
      },
      {
        "command": "okx-wasmy.resetData",
        "title": "%okx-wasmy.resetData.title%",
        "category": "Okx Wasmy"
      },
      {
        "command": "okx-wasmy.build",
        "title": "%okx-wasmy.build.title%",
        "category": "Okx Wasmy"
      },
      {
        "command": "okx-wasmy.runUnitTests",
        "title": "%okx-wasmy.runUnitTests.title%",
        "category": "Okx Wasmy"
      },
      {
        "command": "okx-wasmy.optimizeContract",
        "title": "%okx-wasmy.optimizeContract.title%",
        "category": "Okx Wasmy"
      },
      {
        "command": "okx-wasmy.generateSchema",
        "title": "%okx-wasmy.generateSchema.title%",
        "category": "Okx Wasmy"
      },
      {
        "command": "okx-wasmy.setupDevEnv",
        "title": "%okx-wasmy.setupDevEnv.title%",
        "category": "Okx Wasmy"
      },
      {
        "command": "okx-wasmy.upload",
        "title": "%okx-wasmy.upload.title%",
        "category": "Okx Wasmy",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "okx-wasmy.wasmInteract",
        "title": "%okx-wasmy.wasmInteract.title%",
        "category": "Okx Wasmy",
        "icon": "$(server-process)"
      },
      {
        "command": "okx-wasmy.history",
        "title": "%okx-wasmy.history.title%",
        "category": "Okx Wasmy",
        "icon": "$(history)"
      },
      {
        "command": "okx-wasmy.export",
        "title": "%okx-wasmy.export.title%",
        "category": "Okx Wasmy",
        "shortTitle": "Export Okx Wasmy data"
      },
      {
        "command": "okx-wasmy.addComments",
        "title": "%okx-wasmy.addComments.title%",
        "category": "Okx Wasmy",
        "icon": "$(comment)",
        "enablement": "viewItem == contract"
      },
      {
        "command": "okx-wasmy.queryCosmwasm",
        "title": "%okx-wasmy.queryCosmwasm.title%",
        "category": "Okx Wasmy",
        "enablement": "resourceLangId == json"
      },
      {
        "command": "okx-wasmy.execCosmwasm",
        "title": "%okx-wasmy.execCosmwasm.title%",
        "category": "Okx Wasmy",
        "enablement": "resourceLangId == json"
      },
      {
        "command": "okx-wasmy.beakerTomlSync",
        "title": "%okx-wasmy.beakerTomlSync.title%",
        "category": "Okx Wasmy"
      },
      {
        "command": "okx-wasmy.createCwNotebook",
        "title": "%okx-wasmy.createCwNotebook.title%",
        "category": "Okx Wasmy"
      }
    ],
    "configuration": [
      {
        "title": "Okx Wasmy",
        "properties": {
          "okxwasmy.chainConfigName": {
            "type": "string",
            "markdownDescription": "%okxwasmy.chainConfigName.desc%",
            "scope": "resource",
            "order": 0,
            "default": "Juno UNI-5 testnet",
            "deprecationMessage": "%okxwasmy.chainConfigName.deprecationDesc%"
          },
          "okxwasmy.chains": {
            "type": "array",
            "markdownDescription": "%okxwasmy.chains.desc%",
            "scope": "application",
            "order": 1,
            "minItems": 1,
            "editPresentation": "multilineText",
            "items": {
              "type": "object",
              "title": "%okxwasmy.chains.items.title%",
              "properties": {
                "configName": {
                  "type": "string",
                  "description": "%okxwasmy.chains.items.configName.desc%"
                },
                "chainId": {
                  "type": "string",
                  "description": "%okxwasmy.chains.items.chainId.desc%"
                },
                "chainEnvironment": {
                  "type": "string",
                  "description": "%okxwasmy.chains.items.chainEnvironment.desc%",
                  "enum": [
                    "localnet",
                    "testnet",
                    "mainnet"
                  ],
                  "enumDescriptions": [
                    "%okxwasmy.chains.items.chainEnvironment.localnet.desc%",
                    "%okxwasmy.chains.items.chainEnvironment.testnet.desc%",
                    "%okxwasmy.chains.items.chainEnvironment.mainnet.desc%"
                  ],
                  "default": "testnet"
                },
                "addressPrefix": {
                  "type": "string",
                  "description": "%okxwasmy.chains.items.addressPrefix.desc%"
                },
                "rpcEndpoint": {
                  "type": "string",
                  "description": "%okxwasmy.chains.items.rpcEndpoint.desc%"
                },
                "defaultGasPrice": {
                  "type": "string",
                  "description": "%okxwasmy.chains.items.defaultGasPrice.desc%"
                },
                "chainGasDenom": {
                  "type": "string",
                  "description": "%okxwasmy.chains.items.chainGasDenom.desc%"
                },
                "chainDenomDecimals": {
                  "type": "string",
                  "description": "%okxwasmy.chains.items.chainDenomDecimals.desc%"
                },
                "signType": {
                  "type": "string",
                  "enum": [
                    "tmsecp256k1",
                    "ethsecp256k1"
                  ],
                  "default": "tmsecp256k1",
                  "description": "%okxwasmy.chains.items.signType.desc%",
                  "enumDescriptions": [
                    "%okxwasmy.chains.items.signType.tmsecp256k1.desc%",
                    "%okxwasmy.chains.items.signType.ethsecp256k1.desc%"
                  ]
                },
                "chainDenom": {
                  "type": "string",
                  "description": "%okxwasmy.chains.items.chainDenom.desc%"
                },
                "faucetEndpoint": {
                  "type": "string",
                  "description": "%okxwasmy.chains.items.faucetEndpoint.desc%"
                },
                "accountExplorerLink": {
                  "type": "string",
                  "description": "%okxwasmy.chains.items.accountExplorerLink.desc%",
                  "examples": [
                    "https://testnet.mintscan.io/juno-testnet/account/${accountAddress}"
                  ]
                },
                "txExplorerLink": {
                  "type": "string",
                  "description": "%okxwasmy.chains.items.txExplorerLink.desc%",
                  "examples": [
                    "https://testnet.mintscan.io/juno-testnet/txs/${txHash}"
                  ]
                }
              },
              "required": [
                "configName",
                "chainId",
                "chainEnvironment",
                "addressPrefix",
                "rpcEndpoint",
                "defaultGasPrice",
                "chainDenom"
              ]
            },
            "default": [
              {
                "configName": "Juno uni-6",
                "chainId": "uni-6",
                "chainEnvironment": "testnet",
                "addressPrefix": "juno",
                "rpcEndpoint": "https://juno-testnet-rpc.polkachu.com",
                "defaultGasPrice": "0.025",
                "chainDenom": "ujunox",
                "accountExplorerLink": "https://testnet.mintscan.io/juno-testnet/account/${accountAddress}",
                "txExplorerLink": "https://testnet.mintscan.io/juno-testnet/txs/${txHash}"
              },
              {
                "configName": "Juno",
                "chainId": "testing",
                "chainEnvironment": "localnet",
                "addressPrefix": "juno",
                "rpcEndpoint": "http://localhost:26657",
                "defaultGasPrice": "0.025",
                "chainDenom": "ujunox",
                "faucetEndpoint": "http://localhost:8000"
              },
              {
                "configName": "Osmosis test-4",
                "chainId": "osmo-test-4",
                "chainEnvironment": "testnet",
                "addressPrefix": "osmo",
                "rpcEndpoint": "https://rpc-test.osmosis.zone",
                "defaultGasPrice": "0.025",
                "chainDenom": "uosmo",
                "accountExplorerLink": "https://testnet.mintscan.io/osmosis-testnet/account/${accountAddress}",
                "txExplorerLink": "https://testnet.mintscan.io/osmosis-testnet/txs/${txHash}"
              },
              {
                "configName": "Osmosis",
                "chainId": "localosmosis",
                "chainEnvironment": "localnet",
                "addressPrefix": "osmo",
                "rpcEndpoint": "http://localhost:26657",
                "defaultGasPrice": "0.025",
                "chainDenom": "uosmo"
              },
              {
                "configName": "Archway constantine-2",
                "chainId": "constantine-2",
                "chainEnvironment": "testnet",
                "addressPrefix": "archway",
                "rpcEndpoint": "https://rpc.constantine-2.archway.tech/",
                "defaultGasPrice": "0.025",
                "chainDenom": "const",
                "accountExplorerLink": "https://testnet.mintscan.io/archway-testnet/account/${accountAddress}",
                "txExplorerLink": "https://testnet.mintscan.io/archway-testnet/txs/${txHash}"
              },
              {
                "configName": "Stargaze elgafar-1",
                "chainId": "elgafar-1",
                "chainEnvironment": "testnet",
                "addressPrefix": "stars",
                "rpcEndpoint": "https://rpc.elgafar-1.stargaze-apis.com/",
                "defaultGasPrice": "0.025",
                "chainDenom": "ustars",
                "accountExplorerLink": "https://testnet-explorer.publicawesome.dev/stargaze/account/${accountAddress}",
                "txExplorerLink": "https://testnet-explorer.publicawesome.dev/stargaze/tx/${txHash}"
              },
              {
                "configName": "Neutron baryon-1",
                "chainId": "baryon-1",
                "chainEnvironment": "testnet",
                "addressPrefix": "neutron",
                "rpcEndpoint": "https://rpc.baryon.ntrn.info",
                "defaultGasPrice": "0.025",
                "chainDenom": "untrn",
                "accountExplorerLink": "https://testnet.mintscan.io/neutron-testnet/account/${accountAddress}",
                "txExplorerLink": "https://testnet.mintscan.io/neutron-testnet/txs/${txHash}"
              },
              {
                "configName": "OKTC localnet",
                "chainId": "exchain-67",
                "chainEnvironment": "localnet",
                "addressPrefix": "ex",
                "rpcEndpoint": "http://localhost:26657",
                "defaultGasPrice": "100000000",
                "chainDenom": "okt",
                "chainGasDenom": "wei",
                "chainDenomDecimals": "18",
                "signType": "ethsecp256k1"
              },
              {
                "configName": "OKTC testnet",
                "chainId": "exchain-65",
                "chainEnvironment": "testnet",
                "addressPrefix": "ex",
                "rpcEndpoint": "https://exchaintesttmrpc.okex.org",
                "defaultGasPrice": "100000000",
                "chainDenom": "okt",
                "chainGasDenom": "wei",
                "chainDenomDecimals": "18",
                "signType": "ethsecp256k1",
                "accountExplorerLink": "https://www.oklink.com/cn/oktc-test/address/${accountAddress}",
                "txExplorerLink": "https://www.oklink.com/cn/oktc-test/tx/${txHash}"
              }
            ]
          },
          "okxwasmy.contractSortOrder": {
            "type": "string",
            "enum": [
              "Alphabetical",
              "CodeId",
              "None"
            ],
            "enumDescriptions": [
              "%okxwasmy.contractSortOrder.alphabetical.desc%",
              "%okxwasmy.contractSortOrder.codeId.desc%",
              "%okxwasmy.contractSortOrder.none.desc%"
            ],
            "order": 2,
            "default": "None",
            "description": "%okxwasmy.contractSortOrder.desc%",
            "scope": "resource"
          },
          "okxwasmy.cosmwasmResponseView": {
            "type": "string",
            "enum": [
              "NewFile",
              "Terminal"
            ],
            "enumDescriptions": [
              "%okxwasmy.cosmwasmResponseView.newFile.desc%",
              "%okxwasmy.cosmwasmResponseView.terminal.desc%"
            ],
            "order": 3,
            "default": "Terminal",
            "description": "%okxwasmy.cosmwasmResponseView.desc%",
            "scope": "resource"
          },
          "okxwasmy.maxHistoryStored": {
            "type": "number",
            "markdownDescription": "%okxwasmy.maxHistoryStored.desc%",
            "order": 4,
            "default": 20,
            "minimum": 0,
            "maximum": 500,
            "scope": "resource"
          },
          "okxwasmy.openTxExplorerInVscode": {
            "type": "boolean",
            "markdownDescription": "%okxwasmy.openTxExplorerInVscode.desc%",
            "order": 5,
            "default": false,
            "scope": "resource"
          },
          "beaker.autosync": {
            "type": "boolean",
            "markdownDescription": "%beaker.autosync.desc%",
            "default": true,
            "scope": "machine"
          }
        }
      }
    ],
    "keybindings": [
      {
        "command": "okx-wasmy.build",
        "key": "ctrl+shift+b"
      },
      {
        "command": "okx-wasmy.runUnitTests",
        "key": "ctrl+shift+t"
      },
      {
        "command": "okx-wasmy.optimizeContract",
        "key": "ctrl+shift+o"
      },
      {
        "command": "okx-wasmy.generateSchema",
        "key": "ctrl+shift+g"
      },
      {
        "command": "okx-wasmy.upload",
        "key": "ctrl+shift+u"
      },
      {
        "command": "okx-wasmy.history",
        "key": "ctrl+shift+a"
      }
    ],
    "languages": [
      {
        "id": "cw-notebook",
        "aliases": [
          "CW Notebook"
        ],
        "extensions": [
          ".cwnotebook"
        ],
        "icon": {
          "light": "media/icon.svg",
          "dark": "media/icon.svg"
        }
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "okx-wasmy.upload",
          "when": "resourceExtname == .wasm"
        },
        {
          "command": "okx-wasmy.wasmInteract",
          "when": "resourceExtname == .wasm"
        },
        {
          "command": "okx-wasmy.beakerTomlSync",
          "when": "resourceFilename == Beaker.toml"
        }
      ],
      "editor/context": [
        {
          "command": "okx-wasmy.queryCosmwasm",
          "when": "resourceLangId == json",
          "group": "navigation"
        },
        {
          "command": "okx-wasmy.execCosmwasm",
          "when": "resourceLangId == json",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "okx-wasmy.upload",
          "when": "resourceExtname == .wasm",
          "group": "navigation"
        },
        {
          "command": "okx-wasmy.wasmInteract",
          "when": "resourceExtname == .wasm",
          "group": "navigation"
        },
        {
          "command": "okx-wasmy.queryCosmwasm",
          "when": "resourceLangId == json",
          "group": "navigation"
        },
        {
          "command": "okx-wasmy.execCosmwasm",
          "when": "resourceLangId == json",
          "group": "navigation"
        }
      ],
      "editor/title/context": [
        {
          "command": "okx-wasmy.upload",
          "when": "resourceExtname == .wasm"
        }
      ],
      "view/title": [
        {
          "command": "okx-wasmy.addAccount",
          "when": "view == account",
          "group": "navigation"
        },
        {
          "command": "okx-wasmy.refreshAccount",
          "when": "view == account",
          "group": "navigation"
        },
        {
          "command": "okx-wasmy.addContract",
          "when": "view == contract",
          "group": "navigation"
        },
        {
          "command": "okx-wasmy.history",
          "when": "view == query || view == execute",
          "group": "navigation"
        },
        {
          "command": "okx-wasmy.upload",
          "when": "view == contract",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "okx-wasmy.requestFunds",
          "when": "view == account && showRequestFunds",
          "group": "inline"
        },
        {
          "command": "okx-wasmy.openInExplorer",
          "when": "view == account && showOpenInExplorer",
          "group": "inline"
        },
        {
          "command": "okx-wasmy.copyAddress",
          "when": "view == account || view == contract",
          "group": "cutcopypaste"
        },
        {
          "command": "okx-wasmy.copyMnemonic",
          "when": "view == account",
          "group": "cutcopypaste"
        },
        {
          "command": "okx-wasmy.addComments",
          "when": "view == contract",
          "group": "inline"
        },
        {
          "command": "okx-wasmy.updateContractAdmin",
          "when": "view == contract",
          "group": "modify"
        },
        {
          "command": "okx-wasmy.clearContractAdmin",
          "when": "view == contract",
          "group": "modify"
        },
        {
          "command": "okx-wasmy.getContractChecksum",
          "when": "view == contract",
          "group": "details"
        },
        {
          "command": "okx-wasmy.downloadContractBinary",
          "when": "view == contract",
          "group": "details"
        },
        {
          "command": "okx-wasmy.deleteAccount",
          "when": "view == account",
          "group": "z_delete"
        },
        {
          "command": "okx-wasmy.deleteContract",
          "when": "view == contract",
          "group": "z_delete"
        }
      ]
    },
    "notebooks": [
      {
        "type": "cw-notebook",
        "displayName": "cw-notebook",
        "selector": [
          {
            "filenamePattern": "*.cwnotebook"
          }
        ]
      }
    ],
    "views": {
      "okx-wasmy": [
        {
          "id": "account",
          "name": "Account",
          "type": "tree",
          "visibility": "visible"
        },
        {
          "id": "contract",
          "name": "Contract",
          "type": "tree",
          "visibility": "visible"
        },
        {
          "id": "query",
          "name": "Query",
          "type": "webview",
          "visibility": "visible"
        },
        {
          "id": "execute",
          "name": "Execute",
          "type": "webview",
          "visibility": "visible"
        },
        {
          "id": "sign",
          "name": "Sign",
          "type": "webview",
          "visibility": "visible"
        },
        {
          "id": "migrate",
          "name": "Migrate",
          "type": "webview",
          "visibility": "collapsed"
        },
        {
          "id": "intialize",
          "name": "Initialize",
          "type": "webview",
          "visibility": "collapsed"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "okx-wasmy",
          "title": "Okx Wasmy",
          "icon": "media/icon.svg"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "account",
        "contents": "%okxwasmy.viewsWelcome.account.desc%"
      },
      {
        "view": "contract",
        "contents": "%okxwasmy.viewsWelcome.contract.desc%"
      }
    ],
    "walkthroughs": [
      {
        "id": "okx-wasmy",
        "title": "%okxwasmy.walkthroughs.title%",
        "description": "%okxwasmy.walkthroughs.description%",
        "steps": [
          {
            "id": "openCWView",
            "title": "%okxwasmy.walkthroughs.primary.openCWView.title%",
            "description": "%okxwasmy.walkthroughs.primary.openCWView.description%",
            "completionEvents": [
              "onView:account"
            ],
            "media": {
              "image": "media/icon.png",
              "altText": "Okx Wasmy logo"
            }
          },
          {
            "id": "createKey",
            "title": "%okxwasmy.walkthroughs.primary.createKey.title%",
            "description": "%okxwasmy.walkthroughs.primary.createKey.description%",
            "completionEvents": [
              "onCommand:okx-wasmy.addAccount"
            ],
            "media": {
